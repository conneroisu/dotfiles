{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "nix",
      "args": [
        "run",
        "github:conneroisu/nix-playwright-mcp"
      ],
      "env": {}
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "shadcn-ui": {
      "type": "stdio",
      "command": "pnpx",
      "args": [
        "shadcn@latest",
        "registry:mcp"
      ],
      "env": {}
    }
  }
}
